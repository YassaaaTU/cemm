{
	"name": "nuxt_tailwindv4_template",
	"type": "module",
	"version": "1.4.1",
	"private": true,
	"packageManager": "bun@1.2.6",
	"author": "YassaaaTU aka YasirJumaah",
	"license": "MIT",
	"engines": {
		"node": ">=23"
	},
	"scripts": {
		"dev": "nuxt dev",
		"build": "nuxt build",
		"generate": "nuxt generate",
		"clean": "rm -r .nuxt .output node_modules",
		"preinstall": "bunx only-allow bun",
		"postinstall": "nuxt prepare",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"typecheck": "tsc --noEmit",
		"typecheck:fix": "tsc --noEmit --skipLibCheck",
		"analyze": "nuxt analyze",
		"analyze:bundle": "vite-bundle-analyzer .output/dist/client",
		"tauri": "tauri",
		"app": "tauri dev",
		"app:build": "tauri build",
		"cargo:clean": "cd src-tauri; cargo clean",
		"cargo:check": "cd src-tauri; cargo check",
		"cargo:fix": "cd src-tauri; cargo fix",
		"bump": "bunx bumpp --all --config bump.config.ts"
	},
	"dependencies": {
		"@tauri-apps/plugin-dialog": "~2",
		"@tauri-apps/plugin-fs": "~2",
		"@tauri-apps/plugin-opener": "^2.2.7",
		"@tauri-apps/plugin-process": "^2.2.2",
		"@tauri-apps/plugin-updater": "^2.8.1",
		"nuxt": "^3.16.0",
		"pino": "^9.7.0",
		"tauri-plugin-keyring-api": "^0.1.1",
		"virtua": "^0.41.5",
		"vue": "^3.5.13",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.23.0",
		"@nuxt/devtools": "^2.5.0",
		"@nuxt/eslint": "^1.2.0",
		"@nuxt/icon": "^1.11.0",
		"@nuxt/image": "^1.10.0",
		"@pinia/nuxt": "^0.11.1",
		"@stylistic/eslint-plugin": "^4.2.0",
		"@tailwindcss/vite": "^4.0.17",
		"@tauri-apps/api": "^2.5.0",
		"@tauri-apps/cli": "^2.5.0",
		"@typescript-eslint/eslint-plugin": "^8.28.0",
		"@typescript-eslint/parser": "^8.28.0",
		"@vueuse/nuxt": "^13.0.0",
		"bumpp": "^10.2.0",
		"bundle-analyzer": "^0.0.6",
		"daisyui": "^5.0.43",
		"eslint": "^9.0.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-pino-logger": "^1.0.2",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"nuxt-svgo": "^4.0.15",
		"pinia-plugin-persistedstate": "^4.2.0",
		"tailwindcss": "^4.0.17",
		"typescript": "^5.8.2",
		"vite-bundle-analyzer": "^0.22.3",
		"vite-plugin-eslint2": "^5.0.3"
	}
}