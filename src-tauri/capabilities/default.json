{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "opener:default",
    "keyring:default",
    "fs:default",
    "fs:allow-appdata-write-recursive",
    "fs:allow-appdata-read-recursive",
    "fs:allow-exists",
    "fs:allow-remove"
  ]
}